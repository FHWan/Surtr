-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
orienteering.hex \

SECONDARY_LIST += \
orienteering.lst \

SECONDARY_SIZE += \
orienteering.siz \


# 所有目标
all: orienteering.elf secondary-outputs

# 工具调用
orienteering.elf: $(OBJS) $(USER_OBJS)
	@	@	riscv-none-embed-gcc -march=rv32imacxw -mabi=ilp32 -msmall-data-limit=8 -msave-restore -Os -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-common -Wunused -Wuninitialized  -g -T "C:\Users\Lenovo\Desktop\雪龙越野\orienteering\Ld\Link.ld" -nostartfiles -Xlinker --gc-sections -Wl,-Map,"orienteering.map" --specs=nano.specs --specs=nosys.specs -o "orienteering.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@	@
orienteering.hex: orienteering.elf
	@	riscv-none-embed-objcopy -O ihex "orienteering.elf"  "orienteering.hex"
	@	@
orienteering.lst: orienteering.elf
	@	riscv-none-embed-objdump --all-headers --demangle --disassemble -M xw "orienteering.elf" > "orienteering.lst"
	@	@
orienteering.siz: orienteering.elf
	@	riscv-none-embed-size --format=berkeley "orienteering.elf"
	@	@
# 其他目标
clean:
	-$(RM) $(ASM_UPPER_DEPS)$(OBJS)$(SECONDARY_FLASH)$(SECONDARY_LIST)$(SECONDARY_SIZE)$(ASM_DEPS)$(S_DEPS)$(S_UPPER_DEPS)$(C_DEPS) orienteering.elf
	-@
secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

.PHONY: all clean dependents

-include ../makefile.targets
